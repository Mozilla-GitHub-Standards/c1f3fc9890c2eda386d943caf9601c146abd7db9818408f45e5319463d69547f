{
  "dependencies": {
    "bindings": "^1.3.0",
    "node-gyp": "^3.6.2"
  },
  "scripts": {
    "preinstall": "eslint --fix *.js && clang-format -i src/*.c src/*.h",
    "prepublish": "eslint --fix *.js && clang-format -i src/*.c src/*.h",
    "pretest": "eslint --fix *.js && clang-format -i src/*.c src/*.h && node-gyp rebuild",
    "other-tests": "make test",
    "test": "mocha",
    "lint": "eslint --fix *.js"
  },
  "enginesStrict": true,
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "assume": "^2.1.0",
    "eslint-config-taskcluster": "^3.1.0",
    "mocha": "^5.2.0"
  }
}
